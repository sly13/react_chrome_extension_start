{
  "manifest_version": 2,
  "name": "Trade Helper for Binance",
  "description":
    "With the help of the extension you will see the value in dollars of the purchased crypto currency.",
  "version": "0.01",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["materialize/js/jquery-3.3.1.min.js", "content.js"],
      "css": ["content.css"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    // "default_icon": "icons/icon_64.png",
    "default_title": "Trade Helper for Binance"
  },
  "permissions": ["tabs"]
  //"icons": { "128": "icons/icon_128.png" }
}
